<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>증감 연산자</title>
</head>
<body>

<script type="text/javascript">
// 증감 연산자

let num = 5;

num++; //6 , 후행
document.write("num = " + num + "<br>");

++num; //7 , 선행
document.write("num = " + num + "<br>");

let num2 = num++; //후행, let num2 = num 이휴 num++ 으로 진행
document.write("num = " + num + "&emsp; num2 = " + num2 + "<br>");


let num3 = ++num; //선행
document.write("num = " + num + "&emsp; num2 = " + num2 + "<br>");

</script>
</body>
</html>