<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>JSP를 활용한 구구단 만들기</title>
<style type="text/css">

td {
    padding: 5px;
}

</style>
</head>
<body>
    <h1 align="center">JSP를 활용한 구구단 만들기</h1>
    <hr/>
    
    <table>
<% for(int i=1; i<=9; i++) { %>
    <tr>
    <% for(int j=2; j<=9; j++) { %>
    
        <!-- j와 i를 곱한 결과를 출력 -->
        <td><%= j %> * <%= i %> = <%= j * i %></td>

    <% } // for j %>
    </tr>
<% } // for i %>
        
</table>

</body>
</html>
