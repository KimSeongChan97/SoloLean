<body>
    <h1>3의 배수 찾기</h1>
    <p id="su"></p>
    <p id="dap"></p>
    <script type="text/javascript">
        let num = prompt("몇 까지 3의 배수를 찾을까요?");
		
        for(let i=3; i<=num; i+=3) {
        	document.write(i + ", &emsp;");
        }
        
    </script>
</body>
