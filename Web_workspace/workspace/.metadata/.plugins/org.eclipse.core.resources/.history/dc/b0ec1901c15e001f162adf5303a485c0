/*

 //클래스 선언
public class Calc {
	//변수 추가
	public int x = 0;
	public int y = 0;
	// 메서드
	public void setValue(int x, int y) {
		this.x = x;
		this.y = y;
	}
}

//객체 생성
Calc calc = new Calc(); -> var calc = {}
calc.x = 25;

*/

// 먼저, 빈 객체로 생성
var calc = {}

// 멤버변수(필드) 추가
calc.x = 0;
calc.y = 0;

// 멤버함수(메서드) 추가
calc.setValue = function(x, y) {
	this.x = x;
	this.y = y;
}


