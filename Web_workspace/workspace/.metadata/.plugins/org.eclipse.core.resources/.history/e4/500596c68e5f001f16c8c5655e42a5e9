		function Lotto() { // 생성함수
			const lottoNumbers = new Set(); // 번호 저장
			while (lottoNumbers.size < 6) {
				// 중복X의 6개 로또번호 나올 때까지 반복
				const number = Math.floor(Math.random());
			}
		}
