<!-- 
jQuery 이미지에 마우스가 올라가면 jquery_on.jpg, 내려가면 jquery.jpg 가 되어야 한다.

 -->
 
 <!-- 
 
 // 모든 이미지에 마우스 오버/아웃 이벤트를 연결합니다.
	$('.hover-change').hover(
		function() {
			// 마우스가 이미지 위에 올라갔을 때 실행됩니다.
			// 이미지의 src를 변경하여 _on 버전의 이미지로 바꿉니다.
			let hoverImg = $(this).data('hover'); // data-hover 속성에 저장된 값 가져오기
			$(this).attr('src', hoverImg); // 이미지 src 변경
		}, 
		function() {
			// 마우스가 이미지에서 내려갔을 때 실행됩니다.
			// 원래 이미지로 돌아갑니다.
			let originalImg = $(this).data('original'); // data-original 속성에 저장된 값 가져오기
			$(this).attr('src', originalImg); // 이미지 src 변경
		}
	);
 
  -->