	<div id="gugudan"></div>
		
    <script type="text/javascript">
        for(let i=1; i<=9; i++){
        	let line = "";
        	
            for(let dan=2; dan<=9; dan++){
            	line += `${dan}*${i}=${dan*i}`;
            } // for
            document.getElementById("gugudan").innerHTML += line + "\n";
        }
    </script>
