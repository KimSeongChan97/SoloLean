	@Override
	public void run() {
		
		
	}
	
	
}

//------------------------------
public class ATMMain {

	public static void main(String[] args) {
		// thread 생성
		
		Thread mom = new Thread(); // this 라고 쓰면 안됨, static 영역에서는 this 값이 없음.
		mom.start();
			
		
		

	}

}
